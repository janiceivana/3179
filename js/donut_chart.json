{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "A simple donut chart with embedded data.",
  "data": {
    "url": "https://raw.githubusercontent.com/janiceivana/3179/main/no_of_deaths_by_country_clean.csv"
  },
  "mark": {
    "type": "arc",
    "innerRadius": 50,
    "tooltip": true
  },
  "encoding": {
    "theta": {
      "field": "Count_median",
      "type": "quantitative",
      "aggregate": "sum"
    },
    "color": {
      "field": "WHO Region",
      "type": "nominal",
      "scale": {
        "scheme": "purplered"
      }
    }
  }
}