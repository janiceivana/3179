{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {"url": "https://raw.githubusercontent.com/janiceivana/3179/main/no_of_deaths_by_country_clean.csv"},
  
  "params": [
    {
      "name": "Continent",
      "bind": {
        "input": "select",
        "options": [
          null,
          "Africa",
          "Americas",
          "Eastern Mediterranean",
          "Europe",
          "South-East Asia",
          "Western Pacific"
        ],
        "labels": [
          "Show All",
          "Africa",
          "Americas",
          "Eastern Mediterranean",
          "Europe",
          "South-East Asia",
          "Western Pacific"
        ],
        "name": "WHO Region: "
      }
    }
  ],
  "transform": [
    {
      "filter": "Continent == null || datum['WHO Region'] == Continent"
    }
  ],
  "config": {
    "legend": {"orient": "left"}
  },
  "hconcat":[
    {
      "title": {
        "text": "Trends Number of Death caused by HIV",
      "fontSize": 16,
      "font": "Lora"},
      "width": 400, "height": 500,
      "mark": {"type": "area","stroke":"black"},
      "encoding": {
        "x": {
          "field": "Year",
          "timeUnit":"year",
        "axis":{"title":"Year"}  },
        "y": {
          "aggregate": "sum", 
          "field": "Count_median",
          "sort":"color",
          "title":"Number of Death caused by HIV"
        },
        "color": {
          "field": "WHO Region",
          "type": "nominal",
          "scale": {
            "domain": [
              "Africa",
              "Americas",
              "Eastern Mediterranean",
              "Europe",
              "South-East Asia",
              "Western Pacific"
            ],
            "range": [
              "#fb9a99",
              "#fdbf6f",
              "#cab2d6",
              "#80b1d3",
              "#ffed6f",
              "darkgreen"
            ]
          }
      },
    
      "tooltip": [
        {"field": "Year","timeUnit":"year"},
        {"field": "WHO Region"},
        {"aggregate":"sum","field": "Count_median", "type": "quantitative"}
      ]
    }
  },
  {
    
    "width":400, "height":500,
    "title": {
      "text": "Number of Death caused by HIV",
    "fontSize": 16,
    "font": "Lora"},
    "mark": {
      "type": "boxplot",
      "extent": "min-max"
    },
    "encoding": {
      "x": {"field": "WHO Region", "type": "nominal"},
      "color": {
        "field": "WHO Region",
        "type": "nominal",
        "legend": null,
        "scale": {
          "domain": [
            "Africa",
            "Americas",
            "Eastern Mediterranean",
            "Europe",
            "South-East Asia",
            "Western Pacific"
          ],
            "range": [
              "#fb9a99",
              "#fdbf6f",
              "#cab2d6",
              "#80b1d3",
              "#ffed6f",
              "darkgreen"
            ]
        }
      },
        "y": {
        "field": "Count_median",
        "type": "quantitative",
        "scale": {"zero": false},
        "aggregate":"sum",
        "title":"Number of Death caused by HIV"
      }
    }
  }
  ]
  
  
}

